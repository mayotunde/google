  <body>
    <div id="map" style="width:600px;height:450px"></div>
    <script>
     	
      function initMap() {
    var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 8,
          center: {lat: 7, lng: 3},
	mapTypeId: google.maps.MapTypeId.ROADMAP
        });
	var point = new google.maps.LatLng(7.11,3.20);
	var point2 = new google.maps.LatLng(7.11,3.80);
	
	var marker= new google.maps.Marker({
	position:point,
	 icon:'file:///C:/Users/user/Documents/image and icon files/tipper2.png'
	
	});
marker.setMap(map);

var marker2= new google.maps.Marker({
	position:point2,
        icon:'file:///C:/Users/user/Documents/image and icon files/smile2.png'
      	
	});
marker2.setMap(map);

}

</script>

